{
    "manifest_version": 3,
    "name": "Tab Reaper",
    "version": "1.0",
    "description": "Harvests inactive tabs and sends them to tab purgatory",
    "permissions": ["downloads", "tabs", "alarms", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}